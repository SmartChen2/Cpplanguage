#include <cstdio>
#include <iostream>
#include <queue>
#include <algorithm>
using namespace std;

int N = 2; int M = 3;
int arr[2][3];

int main() {
	fill(arr[0], arr[0] + M, 0);
	fill(arr[1], arr[1] + M, 1);
	for (int i = 0; i < 2; i++) {
		for (int j = 0; j < M; j++) {
			printf ("%d ", arr[i][j]);
		}
		printf("\n");
	}
	printf("\n");
	fill(arr[0], arr[0] + M * N, 2);
	for (int i = 0; i < N; i++) {
		for (int j = 0; j < M; j++) {
			printf ("%d ", arr[i][j]);
		}
		printf("\n");
	}
}
